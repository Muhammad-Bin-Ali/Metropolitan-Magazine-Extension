{
    "manifest_version": 3,
    "name": "Metropolitan Magazine - Undecided",
    "description": "",
    "version": "1.0.0",
    "action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": [
        "tabs",
        "scripting",
        "identity",
        "identity.email"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    }
}